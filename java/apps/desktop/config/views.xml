	<!--  Time series views -->
	<usageView>
		<name>User Usage</name>
		<styleId>line-graph</styleId>
		<desktopShortcut>true</desktopShortcut>
		<autoRefreshFrequency>60000</autoRefreshFrequency>
		<dataType>p2psmon_users</dataType>
		<attributes>
			<attributeName>app_id</attributeName>
			<attributeName>username</attributeName>
		</attributes>
		<metrics>
			<metricName>total</metricName>
		</metrics>
		<labels>
			<sourceField>Source</sourceField>
			<allSources>All Sources</allSources>
			<selectSource>Select source...</selectSource>
			<userField>User</userField>
			<allAttributeValues>-- All --</allAttributeValues>
			<selectUser>Select user...</selectUser>
		</labels>
		<contextMenu>
			<menuItem toolClass="com.prelert.data.ListViewTool">
				<name>Show Evidence</name>
				<viewToOpen>Evidence View</viewToOpen>
			</menuItem>
			<menuItem toolClass="com.prelert.data.UsageViewTool">
				<name>Show Server Usage</name>
				<viewToOpen>Server Usage</viewToOpen>
				<timeFrame>HOUR</timeFrame>
			</menuItem>
			<menuItem toolClass="com.prelert.data.UsageViewTool">
				<name>Show IPC Usage</name>
				<viewToOpen>IPC Usage</viewToOpen>
				<timeFrame>HOUR</timeFrame>
			</menuItem>
			<menuItem toolClass="com.prelert.data.UsageViewTool">
				<name>Show mdhmon</name>
				<viewToOpen>mdhmon</viewToOpen>
				<timeFrame>HOUR</timeFrame>
			</menuItem>
			<menuItem toolClass="com.prelert.data.UsageViewTool">
				<name>Show System UDP</name>
				<viewToOpen>System UDP</viewToOpen>
				<timeFrame>HOUR</timeFrame>
			</menuItem>
		</contextMenu>
	</usageView>
	
