<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

	<display-name>Prelert Diagnostics for CA APM</display-name>
	<description>
	 	Prelert Diagnostics for CA APM
	</description>

	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>/WEB-INF/applicationContext.xml /WEB-INF/securityContext.xml</param-value>
	</context-param>
    
 	<filter>
		<filter-name>filterChainProxy</filter-name>
		<filter-class>org.springframework.web.filter.DelegatingFilterProxy</filter-class>
	</filter>
    	
	<filter-mapping>
		<filter-name>filterChainProxy</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
    
	
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	
	
	<servlet>
		<servlet-name>prelert</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>prelert</servlet-name>
		<url-pattern>/diagnosticsCA/services/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>prelert</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	

    <welcome-file-list>
        <welcome-file>DiagnosticsCAAPM.html</welcome-file>
    </welcome-file-list>
  
    <session-config> 
		<session-timeout>480</session-timeout> 
	</session-config>

</web-app>
