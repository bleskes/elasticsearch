	<!-- Usage Views  -->
	<usageView>
		<name>p2psmon users</name>
		<styleId>line-graph</styleId>
		<desktopShortcut>true</desktopShortcut>
		<autoRefreshFrequency>60000</autoRefreshFrequency>
		<dataType>p2psmon_users</dataType>
		<attributes>
			<attributeName>appId</attributeName>
			<attributeName>username</attributeName>
		</attributes>
		<metrics>
			<metricName>total</metricName>
		</metrics>
		<labels>
			<sourceField>Source</sourceField>
			<allSources>All Sources</allSources>
			<selectSource>Select source...</selectSource>
			<userField>User</userField>
			<allAttributeValues>-- All --</allAttributeValues>
			<selectUser>Select user...</selectUser>
		</labels>
		<contextMenu>
			<menuItem toolClass="demo.app.data.ListViewTool">
				<name>Show p2ps logs</name>
				<viewToOpen>p2ps logs</viewToOpen>
			</menuItem>
			<menuItem toolClass="demo.app.data.ListViewTool">
				<name>Show mdh logs</name>
				<viewToOpen>mdh logs</viewToOpen>
			</menuItem>
			<menuItem toolClass="demo.app.data.UsageViewTool">
				<name>Show p2psmon ipc</name>
				<viewToOpen>p2psmon ipc</viewToOpen>
				<timeFrame>HOUR</timeFrame>
			</menuItem>
			<menuItem toolClass="demo.app.data.UsageViewTool">
				<name>Show mdhmon servers</name>
