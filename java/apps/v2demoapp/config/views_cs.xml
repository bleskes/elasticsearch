	<!-- History View -->
	<historyView>
		<name>History View</name>
		<dataType>p2pslog</dataType>
		<styleId>list-week</styleId>
		<desktopShortcut>false</desktopShortcut>
		<evidenceView viewClass="demo.app.data.EvidenceView">
			<name>Evidence By Day</name>
			<styleId>list-day</styleId>
			<timeFrame>DAY</timeFrame>
			<dbView>evidence_day_view</dbView>
			<defaultOrderBy>
				<sortInfo>
					<columnName>day</columnName>
					<sortDirection>DESC</sortDirection>
				</sortInfo>
			</defaultOrderBy>
		</evidenceView>
		<evidenceView viewClass="demo.app.data.EvidenceView">
			<name>Evidence By Hour</name>
			<styleId>list-hour</styleId>
			<timeFrame>HOUR</timeFrame>
			<dbView>evidence_hour_view</dbView>
			<defaultOrderBy>
				<sortInfo>
					<columnName>hour</columnName>
					<sortDirection>DESC</sortDirection>
				</sortInfo>
			</defaultOrderBy>
		</evidenceView>
		<evidenceView viewClass="demo.app.data.EvidenceView">
			<name>Evidence By Minute</name>
			<styleId>list-min</styleId>
			<timeFrame>MINUTE</timeFrame>
			<dbView>evidence_minute_view</dbView>
			<defaultOrderBy>
				<sortInfo>
					<columnName>minute</columnName>
					<sortDirection>DESC</sortDirection>
				</sortInfo>
			</defaultOrderBy>
		</evidenceView>
		<evidenceView viewClass="com.prelert.data.EvidenceView">
			<name>Evidence By Second</name>
			<styleId>list-sec</styleId>
			<timeFrame>SECOND</timeFrame>
			<dbView>evidence_second_view</dbView>
			<defaultOrderBy>
				<sortInfo>
					<columnName>time</columnName>
					<sortDirection>DESC</sortDirection>
				</sortInfo>
			</defaultOrderBy>
			<contextMenu>
				<menuItem toolClass="demo.app.data.ListViewTool">
					<name>Show Evidence</name>
				</menuItem>
				<menuItem toolClass="demo.app.data.CausalityViewTool">
					<name>Show Probable Cause</name>
					<viewToOpen>Causality View</viewToOpen>
				</menuItem>
				<menuItem toolClass="demo.app.data.UsageViewTool">
					<name>Show p2psmon users</name>
					<viewToOpen>p2psmon users</viewToOpen>
					<timeFrame>HOUR</timeFrame>
					<sourceArg>source</sourceArg>
					<attributeName>username</attributeName>
					<attributeValueArg>username</attributeValueArg>
					<timeArg>time</timeArg>
				</menuItem>
				<menuItem toolClass="demo.app.data.UsageViewTool">
					<name>Show p2psmon ipc</name>
					<viewToOpen>p2psmon ipc</viewToOpen>
					<timeFrame>HOUR</timeFrame>
					<sourceArg>source</sourceArg>
					<timeArg>time</timeArg>
				</menuItem>
				<menuItem toolClass="demo.app.data.UsageViewTool">
					<name>Show mdhmon servers</name>
					<viewToOpen>mdhmon servers</viewToOpen>
					<timeFrame>HOUR</timeFrame>
					<sourceArg>source</sourceArg>
					<attributeName>server</attributeName>
					<attributeValueArg>server</attributeValueArg>
					<timeArg>time</timeArg>
				</menuItem>
				<menuItem toolClass="demo.app.data.UsageViewTool">
					<name>Show system udp</name>
					<viewToOpen>system udp</viewToOpen>
					<timeFrame>HOUR</timeFrame>
					<sourceArg>source</sourceArg>
					<timeArg>time</timeArg>
				</menuItem>
			</contextMenu>
		</evidenceView>
	</historyView>
	
	
	<!-- Causality View  -->
	<causalityView>
		<name>Probable Causes</name>
		<styleId>prob-cause</styleId>
		<desktopShortcut>false</desktopShortcut>
		<displayAsEpisodes>false</displayAsEpisodes>
		<contextMenu>
		</contextMenu>
	</causalityView>
	
	
