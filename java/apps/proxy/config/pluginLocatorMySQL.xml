<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
           
    <import resource="dataSource.xml"/>
    	
    <bean id="pluginLocatorDAO" class="com.prelert.proxy.pluginLocator.PluginLocatorMySQLDAO">
        <property name="dataSource" ref="dataSource" />
    </bean>

    <bean id="pluginLocator" class="com.prelert.proxy.pluginLocator.PluginLocator">
        <property name="pluginLocatorDAO" ref="pluginLocatorDAO" />
    </bean>   

</beans>

