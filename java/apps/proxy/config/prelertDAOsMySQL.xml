<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
           
    <import resource="dataSource.xml"/>                 
    
    <bean id="causalityDAO" class="com.prelert.dao.mysql.CausalityMySQLDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="dataSourceDAO" class="com.prelert.dao.mysql.DataSourceMySQLDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="evidenceDAO" class="com.prelert.dao.mysql.EvidenceMySQLDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

    <bean id="incidentDAO" class="com.prelert.dao.mysql.IncidentMySQLDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="timeSeriesDAO" class="com.prelert.dao.mysql.TimeSeriesMySQLDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
	
	<bean id="userDAO" class="com.prelert.dao.mysql.UserMySQLDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
    
 	<bean id="inputManagerDAO" class="com.prelert.proxy.inputmanager.dao.InputManagerMySQLDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
    
    
    <bean id="databaseManagerDAO" class="com.prelert.proxy.datamanager.dao.DatabaseManagerMySQLDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>
        
</beans>

