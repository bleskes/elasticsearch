{
  "xpack.prelert.get_categories": {
    "methods": [ "GET" ],
    "url": {
      "path": "/_xpack/prelert/results/{jobId}/categorydefinitions",
      "paths": [ "/_xpack/prelert/results/{jobId}/categorydefinitions" ],
      "parts": {
        "jobId": {
          "type" : "string",
          "required": true
        }
      },
      "params": {
        "skip": {
          "type": "int",
          "description": "skips a number of categories"
        },
        "take": {
          "type": "int",
          "description": "specifies a max number of categories to get"
        }
      }
    }
  }
}
